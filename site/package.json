    "luxon": "^3.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
