{
  "name": "tools",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "clean": "node -e \"fs.rmSync('dist', {force:true,recursive:true})\"",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "seed:events": "tsc && node dist/seed-events.js",
    "catchup": "tsc && node dist/catch-up.js",
    "create-stats": "tsc && node dist/create-stats.js",
    "rewrite": "tsc && node dist/rewrite.js"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "dotenv": "^16.4.5",
    "firebase-admin": "^12.1.0",
    "luxon": "^3.4.4",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.19.33",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  }
}
