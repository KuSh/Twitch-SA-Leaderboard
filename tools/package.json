{
  "name": "tools",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "clean": "node -e \"fs.rmSync('dist', {force:true,recursive:true})\"",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "seed:events": "tsc && node dist/seed-events.js",
    "catchup": "tsc && node dist/catch-up.js",
    "create-stats": "tsc && node dist/create-stats.js",
    "rewrite": "tsc && node dist/rewrite.js"
  },
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "dotenv": "^16.4.5",
    "firebase-admin": "^12.7.0",
    "luxon": "^3.5.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.17.6",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  }
}
